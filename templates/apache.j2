<source>
  type tail
  format apache2
  path /var/log/apache2/access.log
  pos_file /var/log/td-agent/ckan.access.log.pos
  tag "{{ system }}.apache.access"
</source>
<source>
  type tail
  format apache_error
  path /var/log/apache2/error.log
  pos_file /var/log/td-agent/access.log.pos
  tag "{{ system }}.apache.error"
</source>